
@model UnimetalWeb.Models.Partitiontypemaster
@{
    ViewData["Title"] = "Details";

}

<section class="content">
    <div class="container-fluid">
        <div class="row">

            <div class="col-md-12">
                <!-- general form elements -->
                <div class="card card-primary">
                    <div class="card-header">
                        <h3 class="card-title">Partition Type Master</h3>
                    </div>
                    <!-- /.card-header -->
                    <!-- form start -->
                    @using (Html.BeginForm())
                    {
                        @Html.AntiForgeryToken()
                        <div class="card-body">
                            <div class="form-group">
                                <label asp-for="Id"></label>
                                <input asp-for="Id" class="form-control" disabled />
                                <span asp-validation-for="Id" class="text-danger"></span>



                            </div>
                            <div class="form-group">

                                <label asp-for="PartitionTypeTitle"></label>
                                <input asp-for="PartitionTypeTitle" class="form-control" />
                                <span asp-validation-for="PartitionTypeTitle" class="text-danger"></span>


                            </div>
                            <div class="form-group">

                                <label asp-for="PartitionTypeAbbreviation"></label>
                                <input asp-for="PartitionTypeAbbreviation" class="form-control" />
                                <span asp-validation-for="PartitionTypeAbbreviation" class="text-danger"></span>

                            </div>

                        </div>
                        <!-- /.card-body -->

                        <div class="card-footer">

                            <a asp-action="Edit" asp-controller="Partitiontypemaster" asp-route-id="@Model.Id" class="btn btn-primary">Edit</a>
                            <a asp-action="Index" class="btn btn-danger">Back to List</a>
                        </div>

                    }

                </div>


            </div>

        </div>
        <!-- /.row -->
    </div><!-- /.container-fluid -->
</section>

<script>
    var liMasters = document.getElementById("liMasters");
    liMasters.classList.add("menu-open");
    var liMastersA = document.getElementById("liMastersA");
    liMastersA.classList.add("active");
    var Master = document.getElementById("Partitiontypemaster");
    Master.classList.add("active");

</script>