@model UnimetalWeb.Models.Traytypemaster

@{
    ViewData["Title"] = Model == null ? "Create New" : "Edit";

}


<hr />


<section class="content">
    <div class="container-fluid">
        <div class="row">

            <div class="col-md-12">
                <!-- general form elements -->
                <div class="card card-primary">
                    <div class="card-header">
                        <h3 class="card-title">Tray Type Master</h3>
                    </div>
                    <!-- /.card-header -->
                    <!-- form start -->
                    @using (Html.BeginForm())
                    {
                        @Html.AntiForgeryToken()
                        <div class="card-body">
                            <div class="form-group">
                                <label asp-for="Id"></label>
                                <input asp-for="Id" class="form-control" disabled />
                                <span asp-validation-for="Id" class="text-danger"></span>



                            </div>
                            <div class="form-group">

                                <label asp-for="TrayTypeTitle"></label>
                                <input asp-for="TrayTypeTitle" class="form-control" />
                                <span asp-validation-for="TrayTypeTitle" class="text-danger"></span>


                            </div>
                            <div class="form-group">

                                <label asp-for="TrayTypeAbbreviation"></label>
                                <input asp-for="TrayTypeAbbreviation" class="form-control" />
                                <span asp-validation-for="TrayTypeAbbreviation" class="text-danger"></span>

                            </div>

                        </div>
                        <!-- /.card-body -->

                        <div class="card-footer">
                            <input type="submit" value="Save" class="btn btn-primary" />
                            <a asp-action="Index" class="btn btn-danger">Back to List</a>
                        </div>

                    }

                </div>


            </div>

        </div>
        <!-- /.row -->
    </div><!-- /.container-fluid -->
</section>
